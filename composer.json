{
    "name": "modules/desa-module-release",
    "description": "Modul Desa Module Release untuk manajemen fitur terkait Desa Module Release di sistem desa digital.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Modules\\DesaModuleRelease\\": "src/"
        }
    },
    "exclude-from-classmap": [
        "installer.php",
        "bootstrap.php"
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "laravel/framework": "^11.0",
        "orchestra/testbench": "^9.14",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.0",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\DesaModuleRelease\\Providers\\ModuleServiceProvider"
            ]
        }
    },
    "require": {
        "spatie/laravel-permission": "^6.20",
        "laravel/socialite": "^5.21"
    }
}